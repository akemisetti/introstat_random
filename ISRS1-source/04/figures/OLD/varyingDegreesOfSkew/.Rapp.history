#dnorm(X) + dunif(X, -2, 2) + dunif(X, -1, 1)/2 + dunif(X, -0.5, 0.5)/4#
plot(X, Y, type="l", xlab="", ylab="", axes=FALSE, xlim=c(-4, 4))#
axis(1, at=-4:4, labels=rep("", 9))#
abline(h=0)#
#
par(mar=c(2,3.2,1,0.3))#
x <- rnorm(10)#
histPlot(x, axes=FALSE, ylab="", xlab="10 observations")#
axis(2, 0:5)#
rug(x)#
